<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".DashBoardLayout">

    <androidx.cardview.widget.CardView
        android:id="@+id/accidentImageAndLableCardViewID"
        android:onClick="onClick"

        android:layout_width="150dp"
        android:layout_height="150dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="@dimen/cardview_compat_inset_shadow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055000012">

        <androidx.cardview.widget.CardView
            android:id="@+id/profileCardViewID"
            android:layout_width="80dp"
            android:layout_height="79dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="18dp"
            app:cardCornerRadius="15dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/profileImageCardViewID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/accidentpng" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/accidentTextViewID"
            android:layout_width="107dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="110dp"
            android:foregroundGravity="center"
            android:text="Accident "
            android:textAlignment="center"
            android:textColor="@color/design_default_color_on_secondary" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/fireImageAndLableCardViewID"
        android:onClick="onClick"

        android:layout_width="150dp"
        android:layout_height="150dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="@dimen/cardview_compat_inset_shadow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055000012">

        <androidx.cardview.widget.CardView
            android:id="@+id/fireImageCardViewID"
            android:layout_width="80dp"
            android:layout_height="79dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="18dp"
            app:cardCornerRadius="15dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/fireImageViewID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:src="@drawable/fireextinguisher2" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/fireTextViewID"
            android:layout_width="107dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="110dp"
            android:foregroundGravity="center"
            android:text="Fire "
            android:textAlignment="center"
            android:textColor="@color/design_default_color_on_secondary" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/medicalImageAndLableCardViewID"
        android:onClick="onClick"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="@dimen/cardview_compat_inset_shadow"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accidentImageAndLableCardViewID"
        app:layout_constraintVertical_bias="0.040000012">

        <androidx.cardview.widget.CardView
            android:id="@+id/medicalImageCardViewID"
            android:layout_width="80dp"
            android:layout_height="79dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="18dp"
            app:cardCornerRadius="15dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/medicalImageViewID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:src="@drawable/medical" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/medicalTextViewID"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="110dp"
            android:foregroundGravity="center"
            android:text="Medical "
            android:textAlignment="center"
            android:textColor="@color/design_default_color_on_secondary" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/crimeImageAndLableCardViewID"
        android:onClick="onClick"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="@dimen/cardview_compat_inset_shadow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.88"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fireImageAndLableCardViewID"
        app:layout_constraintVertical_bias="0.040000012">

        <androidx.cardview.widget.CardView
            android:id="@+id/crimeImageCardViewID"
            android:layout_width="80dp"
            android:layout_height="79dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="18dp"
            app:cardCornerRadius="15dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/crimeImageViewID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:src="@drawable/crimepg" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/crimTextViewID"
            android:layout_width="107dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="110dp"
            android:foregroundGravity="center"
            android:text="Crime "
            android:textAlignment="center"
            android:textColor="@color/design_default_color_on_secondary" />
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/drowningImageAndLableCardViewID"
        android:onClick="onClick"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="@dimen/cardview_compat_inset_shadow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medicalImageAndLableCardViewID"
        app:layout_constraintVertical_bias="0.080">

        <androidx.cardview.widget.CardView
            android:id="@+id/drowningImageCardViewID"
            android:layout_width="80dp"
            android:layout_height="79dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="18dp"
            app:cardCornerRadius="15dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/drowningImageViewID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:src="@drawable/drowningpng" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/drowningTextViewID"
            android:layout_width="107dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="110dp"
            android:foregroundGravity="center"
            android:text="Drowning "
            android:textAlignment="center"
            android:textColor="@color/design_default_color_on_secondary" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/structureCollapseImageAndLableCardViewID"
        android:onClick="onClick"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="@dimen/cardview_compat_inset_shadow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.873"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/crimeImageAndLableCardViewID"
        app:layout_constraintVertical_bias="0.085">

        <androidx.cardview.widget.CardView
            android:id="@+id/structureCollapseImageCardViewID"
            android:layout_width="80dp"
            android:layout_height="79dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="18dp"
            app:cardCornerRadius="15dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/structureCollapseImageViewID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:src="@drawable/buildingcollapsepng1" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/structureCollapseTextViewID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="110dp"
            android:foregroundGravity="center"
            android:text="Building Collapse "
            android:textAlignment="center"
            android:textColor="@color/design_default_color_on_secondary" />
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/leftLoweView"
        android:layout_width="191dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/drowningImageAndLableCardViewID"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/right" />

    <ImageView
        android:id="@+id/rightLoweView"
        android:layout_width="191dp"
        android:layout_height="65dp"
        android:layout_marginStart="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toEndOf="@+id/leftLoweView"
        app:srcCompat="@drawable/left" />

    <ImageView
        android:id="@+id/mapIcon"
        android:layout_width="99dp"
        android:layout_height="53dp"
        android:src="@drawable/map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/leftLoweView"
        app:layout_constraintHorizontal_bias="0.26"
        app:layout_constraintStart_toStartOf="@+id/leftLoweView"
        app:layout_constraintTop_toTopOf="@+id/leftLoweView"
        app:layout_constraintVertical_bias="0.97"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/callIcon"
        android:layout_width="108dp"
        android:layout_height="54dp"
        android:src="@drawable/call"
        app:layout_constraintBottom_toBottomOf="@+id/rightLoweView"
        app:layout_constraintEnd_toEndOf="@+id/rightLoweView"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="@+id/rightLoweView"
        app:layout_constraintTop_toTopOf="@+id/rightLoweView"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>